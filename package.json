{
  "name": "payrox-go-beyond",
  "version": "1.0.0",
  "description": "A comprehensive blockchain deployment and orchestration framework",
  "main": "index.js",
  "bin": {
    "check-epoch": "./bin/check-epoch.js",
    "payrox": "./bin/payrox.js"
  },
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:comprehensive": "npm run test:facet-size && npm run test:orchestrator && npm run test:enhanced && npm run test:route-proof",
    "test:enhanced": "hardhat test test/orchestrator-comprehensive.spec.ts && hardhat test test/manifest-utils-enhanced.spec.ts",
    "test:acceptance": "npm run test:facet-size && npm run test:orchestrator && npm run test:route-proof",
    "test:facet-size": "hardhat test test/facet-size-cap.spec.ts",
    "test:orchestrator": "hardhat test test/orchestrator-comprehensive.spec.ts",
    "test:route-proof": "hardhat run scripts/route-proof-selfcheck.ts --network hardhat",
    "test:create2": "hardhat test test/scripts/create2-utility-enhanced.test.ts",
    "test:io": "hardhat test test/scripts/io-utility-enhanced.test.ts",
    "test:epoch": "hardhat test test/scripts/check-epoch-enhanced.test.ts",
    "test:utilities": "npm run test:create2 && npm run test:io && npm run test:epoch",
    "deploy:sepolia": "npm run pre-deploy:sepolia && hardhat run scripts/deploy.ts --network sepolia",
    "deploy:mainnet": "npm run pre-deploy:mainnet && hardhat run scripts/deploy.ts --network mainnet",
    "deploy:ecosystem": "hardhat run scripts/deploy-complete-ecosystem.ts --network localhost && npm run contracts:bundle",
    "epoch:check": "hardhat run scripts/check-epoch.ts",
    "epoch:check:verbose": "hardhat run scripts/check-epoch.ts -- --verbose",
    "epoch:check:json": "hardhat run scripts/check-epoch.ts -- --json",
    "epoch:check:sepolia": "hardhat run scripts/check-epoch.ts --network sepolia -- --verbose",
    "epoch:check:mainnet": "hardhat run scripts/check-epoch.ts --network mainnet -- --verbose",
    "contracts:bundle": "node scripts/generate-contract-bundle.js",
    "contracts:update": "npm run compile && npm run contracts:bundle",
    "contracts:sync": "node scripts/sync-configurations.js",
    "contracts:fix": "npm run contracts:sync && npm run verify:mapping",
    "contracts:abi-sync": "npm run compile && npm run contracts:bundle && npm run verify:abi",
    "ai:deploy": "hardhat run scripts/payrox-ai-production-demo.ts --network hardhat",
    "ai:deploy:localhost": "hardhat run scripts/payrox-ai-production-demo.ts --network localhost",
    "ai:deploy:sepolia": "hardhat run scripts/payrox-ai-production-demo.ts --network sepolia",
    "ai:oz-analysis": "hardhat run scripts/ai-oz-adaptation-facet-linkage-analysis.ts --network hardhat",
    "analyze:facets": "hardhat run scripts/facet-integration-communication-analysis.ts --network hardhat",
    "visualize:facets": "hardhat run scripts/visualize-facet-communication.ts --network hardhat",
    "upgrade:100": "hardhat run scripts/upgrade-to-100-percent-integration.ts --network hardhat",
    "analyze:pingfacet": "hardhat run scripts/analyze-pingfacet-necessity.ts --network hardhat",
    "remove:pingfacet": "hardhat run scripts/remove-pingfacet-manual.ts --network hardhat",
    "update:ecosystem": "hardhat run scripts/complete-ecosystem-update.ts --network hardhat",
    "validate:ecosystem": "hardhat run scripts/final-ecosystem-validation.ts --network hardhat",
    "update:architecture": "hardhat run scripts/update-architecture-documentation.ts --network hardhat",
    "update:ai-knowledge": "hardhat run scripts/update-ai-architecture-knowledge.ts --network hardhat",
    "ai:system": "hardhat run scripts/deploy-go-beyond.ts --network hardhat",
    "ai:demo": "hardhat run scripts/ai-deployment-demo.ts --network hardhat",
    "ai:refactor-demo": "hardhat run scripts/ai-refactoring-intelligence-demo.ts --network hardhat",
    "ai:status": "hardhat run scripts/ai-status-check.ts --network hardhat",
    "ai:security": "npx hardhat run scripts/ai-security-integration.ts --network hardhat",
    "ai:security:local": "npx hardhat run scripts/ai-security-integration.ts --network localhost",
    "web:test": "node scripts/test-web-integration.js",
    "web:setup": "npm run contracts:bundle && npm run web:test",
    "verify:unified": "node scripts/unified-verification.js",
    "verify:mapping": "node scripts/verify-complete-mapping.js",
    "verify:complete": "npm run verify:unified",
    "verify:abi": "node scripts/verify-abi-sync.js",
    "cleanup:analyze": "node scripts/analyze-repository-bloat.js",
    "cleanup:safe": "echo 'Review cleanup-repository.js before running' && node cleanup-repository.js",
    "cleanup:tools": "node scripts/cleanup-redundant-tools.js",
    "release": "hardhat oneclick-release",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "quality:check": "npm run lint && npm run test:comprehensive && npm run test:utilities && npm run coverage && npm run size",
    "typechain": "hardhat typechain",
    "clean": "hardhat clean",
    "coverage": "hardhat coverage",
    "coverage:check": "hardhat coverage && node scripts/check-coverage.js",
    "size": "hardhat size-contracts",
    "verify": "hardhat verify",
    "sbom": "hardhat run scripts/generate-sbom.ts --network hardhat",
    "sbom:local": "hardhat run scripts/generate-sbom.ts --network localhost",
    "sbom:sepolia": "hardhat run scripts/generate-sbom.ts --network sepolia",
    "sbom:mainnet": "hardhat run scripts/generate-sbom.ts --network mainnet",
    "pre-deploy": "ts-node scripts/pre-deploy.ts",
    "pre-deploy:sepolia": "ts-node scripts/pre-deploy.ts --network=sepolia --verbose",
    "pre-deploy:testnet": "ts-node scripts/pre-deploy.ts --network=sepolia --verbose",
    "pre-deploy:mainnet": "ts-node scripts/pre-deploy.ts --network=mainnet --verbose",
    "production-pipeline": "npx hardhat run scripts/production-pipeline.ts",
    "production-pipeline:sepolia": "npx hardhat run scripts/production-pipeline.ts --network sepolia",
    "payrox:manifest:check": "hardhat payrox:manifest:selfcheck --path",
    "payrox:manifest:verify": "hardhat payrox:manifest:selfcheck --check-facets true --path",
    "payrox:chunk:predict": "hardhat payrox:chunk:predict",
    "payrox:chunk:stage": "hardhat payrox:chunk:stage",
    "payrox:orchestrator:start": "hardhat payrox:orchestrator:start",
    "payrox:workflow:full": "npm run payrox:manifest:check && echo '✅ Manifest verified' && npm run payrox:chunk:predict && echo '✅ Ready for staging'",
    "payrox:signature:init": "npx hardhat run scripts/generate-facetb-init-signature.js",
    "payrox:signature:rotate-governance": "cross-env SIGNATURE_OPERATION=rotate-governance npx hardhat run scripts/generate-facetb-init-signature.js",
    "payrox:signature:rotate-operator": "cross-env SIGNATURE_OPERATION=rotate-operator npx hardhat run scripts/generate-facetb-init-signature.js",
    "payrox:signature:interactive": "npx hardhat run scripts/generate-facetb-init-signature.js",
    "payrox:facet:implement-todos": "ts-node tools/facet-todo-implementer.ts",
    "payrox:facet:register-all": "ts-node tools/facet-manifest-registrar.ts",
    "payrox:facet:register": "ts-node tools/facet-manifest-registrar.ts --dispatcher",
    "payrox:facet:validate-production": "ts-node tools/facet-todo-implementer.ts && ts-node tools/facet-manifest-registrar.ts",
    "payrox:diamond:calculate-selectors": "ts-node tools/diamond-selector-manager.ts calculate",
    "payrox:diamond:prepare-cut": "ts-node tools/diamond-selector-manager.ts prepare-cut",
    "payrox:diamond:validate-compatibility": "ts-node tools/diamond-selector-manager.ts validate",
    "deploy:deterministic": "hardhat run scripts/deployDeterministic.ts --network hardhat",
    "deploy:deterministic:localhost": "hardhat run scripts/deployDeterministic.ts --network localhost",
    "deploy:deterministic:sepolia": "hardhat run scripts/deployDeterministic.ts --network sepolia",
    "deploy:deterministic:mainnet": "hardhat run scripts/deployDeterministic.ts --network mainnet",
    "deploy:predict": "cross-env PREDICT_ONLY=true hardhat run scripts/deployDeterministic.ts --network hardhat",
    "deploy:predict:sepolia": "cross-env PREDICT_ONLY=true hardhat run scripts/deployDeterministic.ts --network sepolia",
    "payrox:diamond:analyze-gas": "ts-node tools/diamond-selector-manager.ts analyze",
    "payrox:diamond:export-selectors": "ts-node tools/diamond-selector-manager.ts export",
    "payrox:production:enhance-all": "npm run payrox:facet:implement-todos && npm run payrox:facet:register-all && npm run payrox:diamond:validate-compatibility",
    "ai:deploy:deterministic": "hardhat run scripts/deployDeterministic-ai-enhanced.ts --network hardhat",
    "ai:deploy:deterministic:localhost": "hardhat run scripts/deployDeterministic-ai-enhanced.ts --network localhost",
    "ai:deploy:deterministic:sepolia": "hardhat run scripts/deployDeterministic-ai-enhanced.ts --network sepolia",
    "ai:deploy:deterministic:mainnet": "hardhat run scripts/deployDeterministic-ai-enhanced.ts --network mainnet",
    "ai:predict:deterministic": "cross-env PREDICT_ONLY=true hardhat run scripts/deployDeterministic-ai-enhanced.ts --network hardhat",
    "ai:adaptive:deploy": "hardhat run scripts/deployDeterministic-adaptive.ts --network hardhat",
    "ai:adaptive:learn": "cross-env AI_LEARNING_ENABLED=true hardhat run scripts/deployDeterministic-adaptive.ts --network hardhat",
    "ai:adaptive:predict": "cross-env PREDICT_ONLY=true AI_LEARNING_ENABLED=true hardhat run scripts/deployDeterministic-adaptive.ts --network hardhat",
    "cross-chain:demo": "hardhat run scripts/demo-deterministic-addressing.ts --network localhost",
    "cross-chain:registry": "hardhat run scripts/create-cross-network-registry.ts --network localhost",
    "deterministic:factory": "hardhat run scripts/deploy-deterministic-factory.ts --network localhost",
    "routes:apply": "hardhat run scripts/apply-all-routes.ts",
    "routes:apply:dry": "hardhat run scripts/apply-all-routes.ts",
    "routes:apply:verbose": "hardhat run scripts/apply-all-routes.ts",
    "routes:apply:force": "hardhat run scripts/apply-all-routes.ts",
    "routes:apply:batch": "hardhat run scripts/apply-all-routes.ts",
    "routes:apply:sepolia": "hardhat run scripts/apply-all-routes.ts --network sepolia",
    "routes:apply:mainnet": "hardhat run scripts/apply-all-routes.ts --network mainnet",
    "production-pipeline:mainnet": "npx hardhat run scripts/production-pipeline.ts --network mainnet",
    "test-pipeline": "npx hardhat run scripts/test-production-pipeline.ts --network hardhat",
    "postverify": "npx hardhat run scripts/postverify.ts",
    "postverify:sepolia": "npx hardhat run scripts/postverify.ts --network sepolia",
    "postverify:testnet": "npx hardhat run scripts/postverify.ts --network sepolia",
    "postverify:mainnet": "npx hardhat run scripts/postverify.ts --network mainnet",
    "build": "npm run compile && npm run sbom",
    "ci": "npm run lint && npm run test && npm run coverage && npm run sbom",
    "deploy": "npm run pre-deploy && hardhat run scripts/deploy.ts",
    "_comment_next_steps": "=== NEXT STEPS AUTOMATION ===",
    "next-steps:init": "npx ts-node scripts/next-steps.ts init",
    "next-steps:create-facet": "npx ts-node scripts/next-steps.ts create-facet",
    "next-steps:deploy-multi": "npx ts-node scripts/next-steps.ts deploy-multi",
    "next-steps:verify": "npx ts-node scripts/next-steps.ts verify",
    "next-steps:monitor": "npx ts-node scripts/next-steps.ts monitor",
    "next-steps:status": "npx ts-node scripts/next-steps.ts status",
    "_comment_facet_integration": "=== FACET INTEGRATION ===",
    "test:chunk-factory-integration": "hardhat run scripts/test-chunk-factory-integration.ts --network localhost",
    "deploy:chunk-factory-facet": "hardhat run scripts/deploy-chunk-factory-facet.ts --network localhost",
    "_comment_cross_network": "=== CROSS-NETWORK DEPLOYMENT ===",
    "deploy:cross-network": "npx ts-node scripts/deploy-cross-network.ts",
    "deploy:all-networks": "npx ts-node scripts/deploy-cross-network.ts all",
    "deploy:specific": "npx ts-node scripts/deploy-cross-network.ts networks",
    "verify:cross-network": "npx ts-node scripts/deploy-cross-network.ts verify",
    "_comment_shortcuts": "=== QUICK ACCESS SHORTCUTS ===",
    "create:facet": "npm run next-steps:create-facet",
    "deploy:multi-network": "npm run next-steps:deploy-multi",
    "verify:deployment": "npm run next-steps:verify",
    "report:system-status": "npx ts-node scripts/system-status.ts",
    "setup:monitoring": "npm run next-steps:monitor",
    "_comment_testing": "=== ENHANCED TESTING ===",
    "test:facets": "npx hardhat test test/facets/**/*.test.ts",
    "test:cross-network": "npm run verify:cross-network",
    "test:comprehensive-next": "npm run test && npm run test:facets && npm run verify:deployment",
    "_comment_auditing": "=== SECURITY & AUDITING ===",
    "audit:facets": "npx hardhat run scripts/audit-facets.ts",
    "audit:security": "npm audit && npx hardhat run scripts/security-check.ts",
    "_comment_monitoring": "=== MONITORING & REPORTING ===",
    "start:dashboard": "cd tools/monitoring && npm start",
    "dev:dashboard": "cd tools/monitoring && npm run dev",
    "report:cross-network": "npx ts-node scripts/generate-cross-network-report.ts",
    "report:deployment": "npx ts-node scripts/generate-deployment-report.ts",
    "_comment_setup": "=== INFRASTRUCTURE SETUP ===",
    "setup:indexing": "npx ts-node scripts/setup-event-indexing.ts",
    "setup:alerts": "npx ts-node scripts/setup-alerting.ts",
    "_comment_cleanup": "=== CLEANUP & MAINTENANCE ===",
    "clean:deployments": "rimraf deployments/* reports/*",
    "clean:cache": "npx hardhat clean && rimraf cache artifacts",
    "clean:all": "npm run clean:deployments && npm run clean:cache"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "smart-contracts",
    "hardhat",
    "typescript",
    "deployment",
    "orchestration"
  ],
  "author": "PayRox Team",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.13",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.14",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.0",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomicfoundation/hardhat-verify": "^2.1.0",
    "@nomicfoundation/ignition-core": "^0.15.13",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.1.0",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "chai": "^4.5.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "hardhat": "^2.26.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^2.3.0",
    "js-yaml": "^4.1.0",
    "sinon": "^21.0.0",
    "solidity-coverage": "^0.8.16",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": "^5.8.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "chalk": "^5.4.1",
    "colors": "^1.4.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.9.0",
    "merkletreejs": "^0.5.2",
    "ora": "^8.2.0",
    "yargs": "^17.7.2"
  }
}
