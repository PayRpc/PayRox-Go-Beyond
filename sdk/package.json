{
  "name": "@payrox/go-beyond-sdk",
  "version": "1.0.0",
  "description": "PayRox Go Beyond SDK - Build deterministic dApps with content-addressed deployment",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "contracts"
  ],
  "scripts": {
    "build": "rollup -c && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "ethereum",
    "blockchain",
    "dapp",
    "deterministic",
    "deployment",
    "content-addressed",
    "payrox",
    "sdk",
    "typescript",
    "web3"
  ],
  "author": "PayRox Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PayRpc/PayRox-Go-Beyond.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/PayRpc/PayRox-Go-Beyond/issues"
  },
  "homepage": "https://payrox.dev",
  "peerDependencies": {
    "ethers": "^6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rollup": "^4.6.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
