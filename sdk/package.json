    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rollup": "^4.6.1",
    "ts-jest": "^29.4.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.9.2"
  },
